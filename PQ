Analyst :=
VAR ReportKey =
    COALESCE (
        SELECTEDVALUE ( 'Report'[Report Key] ),
        SELECTEDVALUE ( Inventory[Report Key] ),
        SELECTEDVALUE ( Activity[Report Key] )
    )
RETURN
LOOKUPVALUE (
    Staff[Report Analyst],
    Staff[Staff Key],
        LOOKUPVALUE (
            Inventory[Staff Key],
            Inventory[Report Key], ReportKey
        )
)
