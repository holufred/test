Analyst :=
VAR ActReportKey =
    SELECTEDVALUE ( Activity[Report Key] )
VAR InvReportKey =
    COALESCE (
        ActReportKey,
        SELECTEDVALUE ( Inventory[Report Key] )
    )
VAR InvStaffKey =
    CALCULATE (
        MAX ( Inventory[Staff Key] ),
        REMOVEFILTERS ( Inventory ),
        Inventory[Report Key] = InvReportKey
    )
RETURN
LOOKUPVALUE (
    Staff[Report Analyst],
    Staff[Staff Key], InvStaffKey
)
